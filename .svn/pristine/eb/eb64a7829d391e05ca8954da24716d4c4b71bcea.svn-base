//单例对象申明,实现
#define SHARED_INSTANCE_DEFINE(className) +(className *)sharedInstance;
#define SHARED_INSTANCE_IMPLEMENTATION(className)                              \
+(className *)sharedInstance {                                               \
static className *_##className = nil;                                      \
if (!_##className) {                                                       \
_##className = [[className alloc] init];                                 \
}                                                                          \
return _##className;                                                       \
}
